@prefix : <http://purl.org/nanopub/temp/np#> .
@prefix hycl: <http://purl.org/petapico/o/hycl#> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix orcid: <https://orcid.org/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

:provenance {
    :assertion prov:wasAttributedTo orcid:0000-0000-0000-0000 .
}

:pubinfo {
    : prov:wasAttributedTo orcid:0000-0000-0000-0000 .
}

:Head {
    : a np:Nanopublication ;
        np:hasAssertion :assertion ;
        np:hasProvenance :provenance ;
        np:hasPublicationInfo :pubinfo .
}

:assertion {
    [] hycl:claims "This is a test of nanopub-python" .

    [] hycl:claims "This is another test of nanopub-python" .
}
